   <string notr="true">QMainWindow {
    background-color: #1e1e1e;
}

QWidget {
    background-color: #1e1e1e;
    color: #dcdcdc;
    font: 10pt &quot;Consolas&quot;;
}

/* Кнопки */
QPushButton {
    background-color: #2d2d2d;
    border: 1px solid #3c3c3c;
    border-radius: 4px;
    padding: 6px 12px;
}
QPushButton:hover {
    background-color: #3c3c3c;
}
QPushButton:pressed {
    background-color: #007acc;
    color: white;
    border: 1px solid #007acc;
}

/* Поля ввода */
QLineEdit, QPlainTextEdit, QTextEdit {
    background-color: #252526;
    border: 1px solid #3c3c3c;
    border-radius: 3px;
    padding: 4px;
    color: #dcdcdc;
    selection-background-color: #264f78;
}

/* Таблицы / списки */
QTableWidget, QTreeWidget, QListWidget {
    background-color: #1e1e1e;
    border: 1px solid #3c3c3c;
    gridline-color: #3c3c3c;
}
QHeaderView::section {
    background-color: #2d2d2d;
    color: #cccccc;
    padding: 4px;
    border: 1px solid #3c3c3c;
}

/* Панели/группы */
QGroupBox {
    border: 1px solid #3c3c3c;
    margin-top: 10px;
    padding: 6px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 6px;
    color: #bbbbbb;
    font-weight: bold;
}

/* Вкладки */
QTabWidget::pane {
    border: 1px solid #3c3c3c;
    background: #252526;
}
QTabBar::tab {
    background: #2d2d2d;
    padding: 6px 12px;
    border: 1px solid #3c3c3c;
}
QTabBar::tab:selected {
    background: #007acc;
    color: white;
}

/* Прогрессбары */
QProgressBar {
    border: 1px solid #3c3c3c;
    border-radius: 3px;
    text-align: center;
    background: #252526;
    color: #dcdcdc;
}
QProgressBar::chunk {
    background-color: #007acc;
}
