QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #0a1929, stop:0.5 #0c2b44, stop:1 #0a1929);
}
QPushButton {
    /* ВЫСОКИЕ и УЗКИЕ кнопки */
    min-height: 90px;       /* Высокие */
    min-width: 120px;       /* Узкие */
    max-height: 100px;
    max-width: 140px;
    
    /* Вытянутая вертикальная форма */
    padding: 12px 10px;     /* Больше по вертикали, меньше по горизонтали */
    margin: 4px 8px;        /* Горизонтальные отступы больше */
    
    /* Вертикальное выравнивание текста */
    text-align: center;
    
    /* Стиль */
    background: rgba(255, 255, 255, 0.08);
    backdrop-filter: blur(8px);
    border-radius: 10px;
    
    /* Текст */
    font-family: 'Segoe UI', system-ui;
    font-size: 12px;
    font-weight: 600;
    color: rgba(255, 255, 255, 0.95);
    line-height: 1.3;       /* Межстрочный интервал */
    
    /* Границы - вертикальные градиенты */
    border: 1.5px solid transparent;
    border-image: linear-gradient(to bottom, 
        rgba(0, 180, 255, 0.5), 
        rgba(100, 70, 255, 0.5)
    ) 1;
    
    /* Тени для вертикального акцента */
    box-shadow: 
        inset 0 1px 0 rgba(255, 255, 255, 0.1),
        0 4px 12px rgba(0, 0, 0, 0.25),
        0 1px 2px rgba(0, 0, 0, 0.15);
    
    transition: all 0.25s ease;
}
QPushButton:hover {
    background: rgba(255, 255, 255, 0.12);
    transform: translateY(-2px);
    box-shadow: 
        0 8px 20px rgba(0, 100, 255, 0.3),
        0 2px 4px rgba(0, 0, 0, 0.2);
    border: 1.5px solid rgba(0, 180, 255, 0.7);
}

QPushButton:pressed {
    transform: translateY(0px);
    background: rgba(0, 120, 215, 0.2);
    box-shadow: 
        inset 0 2px 4px rgba(0, 0, 0, 0.25),
        0 1px 3px rgba(0, 0, 0, 0.15);
}

/* ===== КНОПКИ С МНОГОСТРОЧНЫМ ТЕКСТОМ ===== */
QPushButton[multiline=&quot;true&quot;] {
    min-height: 85px;
    min-width: 110px;
    padding: 8px 6px;
    font-size: 11px;
    line-height: 1.2;
}

/* ===== ВЕРТИКАЛЬНЫЕ ГРАДИЕНТЫ ДЛЯ РАЗНЫХ КАТЕГОРИЙ ===== */
/* Синий градиент сверху вниз */
QPushButton[category=&quot;management&quot;] {
    border-image: linear-gradient(to bottom, 
        #00b4ff, 
        #0080ff
    ) 1;
    background: linear-gradient(to bottom,
        rgba(0, 180, 255, 0.1),
        rgba(0, 128, 255, 0.05)
    );
}

/* Фиолетовый градиент */
QPushButton[category=&quot;administration&quot;] {
    border-image: linear-gradient(to bottom, 
        #8a2be2, 
        #4b0082
    ) 1;
    background: linear-gradient(to bottom,
        rgba(138, 43, 226, 0.1),
        rgba(75, 0, 130, 0.05)
    );
}
QPushButton[iconPosition=&quot;top&quot;] {
    padding: 15px 8px 10px 8px;
}
QLabel {
    font-size: 12px;
    padding: 3px;
}

QLabel#title {
    font-size: 24px;       /* Меньше заголовок */
    padding: 15px;
}

QLabel#subtitle {
    font-size: 13px;
    color: rgba(255, 255, 255, 0.6);
    padding: 5px;
}

QMenuBar {
    background-color: rgba(16, 18, 27, 0.7);
    backdrop-filter: blur(10px);
    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
    padding: 8px;
    font-family: 'Segoe UI', 'Arial';
    font-weight: 500;
}

QMenuBar::item {
    background: transparent;
    color: rgba(255, 255, 255, 0.7);
    padding: 8px 20px;
    margin: 0 2px;
    border-radius: 6px;
    font-size: 13px;
    letter-spacing: 0.3px;
}

QMenuBar::item:selected {
    background: rgba(255, 255, 255, 0.12);
    color: #ffffff;
    border: 1px solid rgba(255, 255, 255, 0.2);
    box-shadow: 0 4px 15px rgba(0, 200, 255, 0.2);
}

QMenuBar::item:pressed {
    background: rgba(255, 255, 255, 0.18);
    box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.3);
}

/* ===== ВЫПАДАЮЩИЕ МЕНЮ ===== */
QMenu {
    background-color: rgba(22, 25, 37, 0.95);
    backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.15);
    border-radius: 12px;
    padding: 8px;
    color: #ffffff;
    font-size: 13px;
    box-shadow: 0 25px 50px -12px rgba(0, 0, 0, 0.5);
}

QMenu::item {
    padding: 10px 25px 10px 15px;
    margin: 2px;
    border-radius: 8px;
    background-color: transparent;
    color: rgba(255, 255, 255, 0.8);
    min-height: 28px;
}

QMenu::item:selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 rgba(0, 150, 255, 0.3), stop:1 rgba(100, 50, 255, 0.3));
    color: #ffffff;
    border-left: 3px solid #00b4ff;
}

QMenu::separator {
    height: 1px;
    background: rgba(255, 255, 255, 0.1);
    margin: 6px 10px;
}
</string>
