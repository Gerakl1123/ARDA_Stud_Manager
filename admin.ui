   <string notr="true">QWidget {
    background-color: #2B2B2B;
    color: #E0E0E0;
    font-family: 'Segoe UI', Arial, sans-serif;
    font-size: 14px;
}
/* Заголовки */
QLabel#titleLabel {
    font-size: 22px;
    font-weight: bold;
    color: #FFFFFF;
}

/* Кнопки */
QPushButton {
    background-color: #3A3F44;
    color: #FFFFFF;
    border: 1px solid #555555;
    border-radius: 5px;
    padding: 8px 16px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #50565C;
}

QPushButton:pressed {
    background-color: #1E90FF;
}

/* QLineEdit и QTextEdit */
QLineEdit, QTextEdit {
    background-color: #3A3F44;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 5px;
    color: #FFFFFF;
}

/* QComboBox */
QComboBox {
    background-color: #3A3F44;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 5px;
    color: #FFFFFF;
}

QComboBox QAbstractItemView {
    background-color: #3A3F44;
    selection-background-color: #1E90FF;
    color: #FFFFFF;
}

/* QTableView */
QTableView {
    background-color: #323232;
    gridline-color: #555555;
    color: #FFFFFF;
    selection-background-color: #1E90FF;
    selection-color: #FFFFFF;
    font-size: 13px;
}

QHeaderView::section {
    background-color: #3A3F44;
    color: #FFFFFF;
    padding: 4px;
    border: 1px solid #555555;
    font-weight: bold;
}

/* QCheckBox */
QCheckBox {
    color: #E0E0E0;
}

/* QTabWidget */
QTabWidget::pane {
    border: 1px solid #555555;
}

QTabBar::tab {
    background-color: #3A3F44;
    color: #FFFFFF;
    padding: 8px 16px;
    border: 1px solid #555555;
    border-bottom-color: #2B2B2B;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #1E90FF;
    color: #FFFFFF;
    border-bottom-color: #1E90FF;
}

/* ScrollBar */
QScrollBar:vertical {
    border: none;
    background: #2B2B2B;
    width: 12px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #50555A;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover {
    background: #1E90FF;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}</string>
